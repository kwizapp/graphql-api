{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "typings:generate": "ts-node src/generate-typings",
    "typings:watch": "WATCH_MODE=true npm run typings:generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,test}/**/*.{js,ts,tsx}\"",
    "format:ci": "prettier --list-different \"{src,test}/**/*.{js,ts,tsx}\"",
    "start:nest": "nest start",
    "dev": "npm run typings:generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint:ci": "eslint \"{src,test}/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"{src,test}/**/*.{js,ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.0.1",
    "@nestjs/core": "7.0.1",
    "@nestjs/graphql": "7.0.1",
    "@nestjs/platform-express": "7.0.5",
    "apollo-server-express": "2.11.0",
    "axios": "0.19.2",
    "dotenv": "8.2.0",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@kwizapp/eslint-config-ts": "0.0.8",
    "@kwizapp/prettier-config": "0.0.8",
    "@nestjs/cli": "7.0.0",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.1",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "prettier": "2.0.2",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.2",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.ts"
    ]
  }
}
