{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "typings:generate": "ts-node src/generate-typings",
    "typings:watch": "WATCH_MODE=true npm run typings:generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:nest": "nest start",
    "dev": "npm run typings:generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.0.1",
    "@nestjs/core": "7.0.1",
    "@nestjs/graphql": "7.0.14",
    "@nestjs/platform-express": "7.0.1",
    "apollo-server-express": "2.11.0",
    "dotenv": "8.2.0",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "7.0.0",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.1",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
